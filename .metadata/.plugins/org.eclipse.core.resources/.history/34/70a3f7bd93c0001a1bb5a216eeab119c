package sorting;



public class InsertionSort {
   //these type of sorting need constant extra space 
	
	
	public static void main(String[] args) {
		
		int arr[]= {2,1,3,4,6,5};
		
		
		for(int i=1; i<arr.length;i++) {
			
			int j=i-1;
			if(arr[i]>arr[j]) {
				int temp=arr[i];
				arr[i]=arr[j];
				arr[j]=temp;
			}
			
			
		}
		
		for(int a:arr)
		System.out.print(a);
	}

}
