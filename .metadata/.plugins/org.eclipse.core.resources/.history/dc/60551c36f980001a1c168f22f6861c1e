package arrays;

public class leftRotateByD {

	public static void main(String[] args) {
		
		int a[]= {1,2,3,4,5};
		int d=2;
        leftRotateByNd(a,d); 
	}
   
	
	public static int[] leftRotateByNd(int a[], int d) {
		
		for(int i=1; i<a.length; i++) {
			  a[i-1]=a[i+d];
		}
		
		
		return a;
	}
}
