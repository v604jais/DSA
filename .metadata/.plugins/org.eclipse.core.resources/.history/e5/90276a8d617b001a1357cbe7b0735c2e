package arrays;

public class ArraysMemorayAddress {

	public static void main(String[] args) {
		
		
		
		/*
		 * fixed size array register in stack area in c and c++ while always in heap area in java
		 * to get y element from arr address say x we need to add ysize to address x..i.e x+y
		 * 
		 */
		int arr[]= new int[0];
		System.out.println(arr);

	}

}
