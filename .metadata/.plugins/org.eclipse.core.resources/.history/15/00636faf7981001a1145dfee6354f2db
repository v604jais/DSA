package arrays;

import java.util.Arrays;
import java.util.Collections;
import java.util.List;

public class LeftRotateByM {

	public static void main(String[] args) {
		

		int a[]= {1,2,3,4,5};
		int d=2;
		int size=a.length;
        leftRotateByM(a,0,d); 
        leftRotateByM(a,d,size-1);
        leftRotateByM(a,0,size-1);
        for(int i=0; i<a.length;i++) {
			System.out.println(a[i]);
		}

	}
	 
	 public static int[] leftRotateByM(int a[],int low, int high) {
		
		 Integer[] m= new Integer[a.length];
		 for(int i=0; i <a.length; i++) {
			  m[i]=Integer.valueOf(a[i]);
		 }
		 List<Integer> list= Arrays.asList(m);
		 while(low<high) {
			 
		    Collections.swap(list, low, high);
		    low++;
		    high--;
		 }
		 
		 return a;
	 }

}
