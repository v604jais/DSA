package competetive;

public class TataCliq2 {

	public static void main(String[] args) {
		
		int [] h = {15,15,15};
		int [] p =  {16,18,1};
		int n=1;
		int sum;
		for(int i=1; i<h.length;i+=n) {
			
			if(h[i-1]==h[i] && h[i]==h[i+1]) {
				if(p[i-1]>p[i] && p[i-1]>p[i+1])
					sum=p[i-1];
			}
		}

	}

}
