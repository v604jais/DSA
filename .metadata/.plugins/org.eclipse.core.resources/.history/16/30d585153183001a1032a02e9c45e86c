package arrays;

import java.util.Scanner;

public class GoogleInterview {

	public static void main(String[] args) {
		
		//System.out.println(i+"+"+i+"="+sum);
		Scanner input= new Scanner(System.in);
		int n = input.nextInt(); 
		int sum=0;
		for (int i=1; i<=n;i++) {
			
			for(int j=1;j<=i;j++) {
				sum+=j;
				System.out.printf("%d",sum);
				
			}
			System.out.println();
		}
	}

}
