package sorting;



public class InsertionSort {
   //these type of sorting need constant extra space 
	
	
	public static void main(String[] args) {
		
		int arr[]= {5, 1, 4, 2, 8};
		
		
		for(int i=1; i<arr.length;i++) {
			
			int j=i-1;
			if(arr[j]>arr[i]) {
				int temp=arr[i];
				arr[i]=arr[j];
				arr[j]=temp;
			}
			
			
		}
		
		for(int a:arr)
		System.out.print(a);
	}

}
