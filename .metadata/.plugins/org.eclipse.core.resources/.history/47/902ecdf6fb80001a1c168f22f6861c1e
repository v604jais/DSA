package arrays;

public class leftRotateByD {

	public static void main(String[] args) {
		
		int a[]= {1,2,3,4,5};
		int d=2;
        leftRotateByNd(a,d); 
        
        for(int i=0; i<a.length;i++) {
			System.out.println(a[i]);
		}
	}
   
	
	public static int[] leftRotateByNd(int a[], int d) {
		int count=0;
		for(int i=0; i<a.length; i++) {
			if(i+d<a.length)
			  a[count++]=a[i+d];
		}
		
		
		return a;
	}
}
