package arrays;

public class MaximumCircularSubArraySum {

	public static void main(String[] args) {
		
		int a[]= {1,-2,1,2-2,3};
		
		for(int i=0;i<a.length;i++) {
			
			int max_sum=0,curr_sum=0;
			curr_sum+=a[i];
			max_sum=Math.max(max_sum, curr_sum);
			System.out.println(max_sum+ " : "+curr_sum);
		}
		
		
		

	}

}
